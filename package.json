{
  "name": "@stately-cloud/client",
  "version": "0.29.0",
  "author": "Stately Cloud <support@stately.cloud> (https://stately.cloud/)",
  "description": "Client for StatelyDB, a document database built on top of DynamoDB with an elastic schema that allows you to change your data model any time with automatic backwards compatibility.",
  "types": "dist/index.d.ts",
  "module": "dist/index.js",
  "type": "module",
  "files": [
    "package.json",
    "README.md",
    "LICENSE.txt",
    "dist/**"
  ],
  "homepage": "https://docs.stately.cloud/clients/javascript/",
  "repository": {
    "type": "git",
    "url": "https://github.com/StatelyCloud/js-sdk.git"
  },
  "keywords": [
    "database",
    "nosql",
    "schema"
  ],
  "engines": {
    "node": ">=18.20"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": false,
  "scripts": {
    "build": "mkdir -p dist && cp -r src/api dist && tsc -p tsconfig-build.json",
    "fix": "pnpm run \"/^fix:.*/\"",
    "fix:prettier": "prettier --write 'src/**/*.ts'",
    "lint:prettier": "prettier 'src/**/*.ts'",
    "lint": "eslint 'src/**/*.ts'",
    "fix:eslint": "eslint --fix 'src/**/*.ts'",
    "pretest": "mkdir -p dist-test && cp -r src/api dist-test && tsc -p tsconfig.json",
    "test": "node --test --enable-source-maps"
  },
  "license": "Apache-2.0",
  "devDependencies": {
    "@connectrpc/connect-fastify": "^2.0.0",
    "@stately-cloud/eslint": "workspace:*",
    "@types/node": "^22.4.1",
    "eslint": "^9.9.0",
    "expect": "^29.7.0",
    "fastify": "^5.1.0",
    "typescript": "^5.5.4"
  },
  "dependencies": {
    "@bufbuild/protobuf": "^2.2.0",
    "@connectrpc/connect": "^2.0.0",
    "@connectrpc/connect-node": "^2.0.0"
  },
  "peerDependencies": {
    "@bufbuild/protobuf": "^2.2.0"
  }
}
